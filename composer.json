{
  "name": "your-vendor/docx-converter",
  "description": "Convert DOCX documents to structured data (HTML, JSON, etc.) with enhanced table and style handling.",
  "type": "library",
  "require": {
    "php": ">=8.0",
    "phpoffice/phpword": "^1.1",
    "symfony/console": "^6.0",
    "symfony/yaml": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "DocxConverter\\": "docx-converter/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DocxConverter\\Tests\\": "docx-converter/tests/"
    }
  },
  "bin": [
    "docx-converter/bin/docx-converter"
  ]
}
